<table width="100%">
	<tr>
		<td>
			<div class="field">
				<%= f.label :make %><br>
				<%= f.text_field :make, :required => true, :placeholder => 'Make'%>
			</div>
		</td>
		<td>
			<div class="field">
				<%= f.label :model %><br>
				<%= f.text_field :model, :required => true, :placeholder => 'Model'%>
			</div>
		</td>
		<td>
			<div class="field">
				<%= f.label :year %><br>
				<%= f.text_field :year, :required => true, :digits => true, :placeholder => 'Year' %>
			</div>
		</td>
	</tr>
	<tr>
		<td>
			<div class="field">
				<%= f.label :plate %><br>
				<%= f.text_field :plate , :placeholder => 'Plate #' %>
			</div>
		</td>
		<td>
			<div class="field">
				<%= f.label :state %><br>
				<%= f.text_field :state, :textonly => true, :placeholder => 'State'%>
			</div>
		</td>
		<td>
			<div class="field">
				<%= f.label :color %><br>
				<%= f.text_field :color, :required => true, :textonly => true, :placeholder => 'Color'%>
			</div>
		</td>
	</tr>
	<tr>
		<td>
			<div class="field">
				<%= f.label :owner %><br>
				<%= f.text_field :owner, :placeholder => 'Owner'%>
			</div>
		</td>
	</tr>
</table>